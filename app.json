      "package": "com.thepickleco.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#2A62A2",
      "androidMode": "default",
      "androidCollapsedTitle": "Pickle Co Reminders"
    },
    "plugins": [],
    "extra": {
      "eas": {
